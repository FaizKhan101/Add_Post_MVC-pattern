<header id="main-header">
  <nav>
    <ul>
      <li><a href="/">Home</a></li>
      <% if (!locals.isAuth) { %>
        <li><a href="/signup">Sign-Up</a></li>
        <li><a href="/login">Login</a></li>
      <% } %>
      <% if (locals.isAuth) { %>
        <li><a href="/admin">Admin</a></li>
        <form action="/logout" method="POST">
          <li><button id="logout" type="submit" >Log-Out</button></li>
        </form>
      <% } %>
    </ul>
  </nav>
</header>
